# Standard library module for common math uses

max = { takes x, y to
	if x > y {
		return x
	} else {
		return y
	}
}

min = { takes x, y to
	if max(x, y) == x {
		return y
	} else {
		return x
	}
}

power = { takes n, exp to
	#if exp < 0 {
	#	return 1 / power(n, -1 * exp)
	#}
	if exp == 0 {
		return 1
	}
	result = n
	i = 1
	while i < exp {
		result = result * n
		i = i + 1
	}
	return result
}