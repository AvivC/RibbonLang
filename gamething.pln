map = {takes string, function to
	new_string = ""
	i = 0
	while i < string.length() {
		char = string[i]
		new_string = new_string + function(char)
		i = i + 1
	}
	return new_string
}

char_to_upper = {takes char to
	if char == "a" {
		return "A"
	} elsif char == "b" {
		return "B"
	} elsif char == "c" {
		return "C"
	} elsif char == "d" {
		return "D"
	} elsif char == "e" {
		return "E"
	} elsif char == "f" {
		return "F"
	} elsif char == "g" {
		return "G"
	} elsif char == "h" {
		return "H"
	} elsif char == "i" {
		return "I"
	} elsif char == "j" {
		return "J"
	} elsif char == "k" {
		return "K"
	} elsif char == "l" {
		return "L"
	} elsif char == "m" {
		return "M"
	} elsif char == "n" {
		return "N"
	} elsif char == "o" {
		return "O"
	} elsif char == "p" {
		return "P"
	} elsif char == "q" {
		return "Q"
	} elsif char == "r" {
		return "R"
	} elsif char == "s" {
		return "S"
	} elsif char == "t" {
		return "T"
	} elsif char == "u" {
		return "U"
	} elsif char == "v" {
		return "V"
	} elsif char == "w" {
		return "W"
	} elsif char == "x" {
		return "X"
	} elsif char == "y" {
		return "Y"
	} elsif char == "z" {
		return "Z"
	} else {
		return char
	}
}

string_to_upper = {takes string to
	return map(string, char_to_upper)
}

