# Standard library module for path operations and utilities

concat = { | paths |
    result = ""
    i = 0
    for path in paths {
        result += path
        if i < paths.length() - 1 {
            result += "\"
        }
        i += 1
    }
    return result
}

relative_to_main_directory = { | path |
    return concat([get_main_file_directory(), path])
}
