test binary_operations
    print(10 + 5)
    print(10 - 5)
    print(10 * 5)
    print(10 / 5)
    print(10.0 / 5.0)
    print(10 / 5.0)
    print(-10 + 5)
expect
    15
    5
    50
    2
    2
    2
    -5
end

test expression
    print(10 + 5 / 2)
    print((10 + 5) / 2)
    print((10 + 5) * 2 + -5)
    print((10 + 5) * (2 + -5) / 10)
    print((10 + 5.0) * (2 + -5) / 10)
expect
    12.5
    7.5
    25
    -4.5
    -4.5
end