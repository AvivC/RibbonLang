test for loop
    for i in [1, 2, 3] {
        print(i)
    }

    for i in [1, 2, 3]
    {
        print(i)
    }

    for i in [1, 2, 3] { print(i) }

expect
    1
    2
    3
    1
    2
    3
    1
    2
    3
end

