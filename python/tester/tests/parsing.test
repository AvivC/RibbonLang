test while
    i = 0
    while i < 2 {
        print("a")
        i += 1
    }

    i = 0
    while i < 2 
    {
        print("a")
        i += 1
    }

    i = 0
    while i < 2 { print("a")
        i += 1
    }
expect
    a
    a
    a
    a
    a
    a
end

